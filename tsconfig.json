    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "declaration": true,
        "removeComments": true
    },
    "exclude": ["node_modules"]
}